<!doctype html>
<html>
  <head>
    <title>Welcome to <%=shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1><%=shopName%> Shop</h1>
    <h2>Here are the users subscribed to our shop:</h2>
    <ul>
      <% availableUser.forEach(function(user) { %>
        <li><%= user.username %>, <%= user.firstname %>, <%= user.lastname %></li>
      <% }) %>
    </ul>
    </body>
</html>